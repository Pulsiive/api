{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "db-dev-test-up": "npm run db-up && npm run db-test-up",
    "db-up": "docker-compose up -d postgres && npm run generate-db-models",
    "db-test-up": "docker-compose up -d postgres_test && npm run generate-db-test-models",
    "generate-db-models": "npx prisma migrate dev --name init",
    "generate-db-test-models": "npx dotenv -e .env.test -- npm run generate-db-models",
    "test": "npx dotenv -e .env.test -- npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pulsiive/api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pulsiive/api/issues"
  },
  "homepage": "https://github.com/Pulsiive/api#readme",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "dotenv-cli": "^5.1.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "prettier": "2.6.1",
    "prisma": "^3.12.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*__mocks__.*"
    ]
  }
}